    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/brennanbrown/meddler/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@berryhouse/core": "1.0.2",
    "commander": "^12.1.0",
    "chalk": "^4.1.2",
